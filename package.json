{
  "name": "nestjs-project-template",
  "version": "1.0.0",
  "description": "",
  "author": "seonggukchoi",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development nest start --watch",
    "start:dev": "./node_modules/pm2/bin/pm2-runtime ./pm2/application.development.config.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.3",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.19.2",
    "mysql": "^2.18.1",
    "pm2": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.29",
    "typeorm-transactional-cls-hooked": "^0.1.17",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.5.5",
    "@types/config": "0.0.37",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.17.8",
    "@types/jest": "26.0.18",
    "@types/node": "^14.14.11",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
